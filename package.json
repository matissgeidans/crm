{
  "name": "rest-express",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"tsx server/index.ts\" \"vite\"",
    "start": "NODE_ENV=production tsx server/index.ts",
    "build": "vite build && echo 'Server build not needed, using tsx directly'",
    "check": "tsc",
    "db:push": "drizzle-kit push"
  },
  "dependencies": {
    "concurrently": "^8.2.0",
    "drizzle-kit": "^0.12.1",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "pg": "^8.12.0",
    "uuid": "^9.0.0",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "tsx": "^4.16.7",
    "typescript": "^5.3.0",
    "vite": "^5.4.20",
    "@types/express": "^4.17.21",
    "@types/express-session": "^1.17.6",
    "@types/node": "^20.5.6"
  }
}
